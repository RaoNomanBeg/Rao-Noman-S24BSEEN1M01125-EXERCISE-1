def create_new_list(list1, list2):
    new_list = []
    # Add odd numbers from the first list
    for num in list1:
        if num % 2 != 0:
            new_list.append(num)
    # Add even numbers from the second list
    for num in list2:
        if num % 2 == 0:
            new_list.append(num)
    return new_list

# Example usage:
list1 = [1, 2, 3, 4, 5]
list2 = [6, 7, 8, 9, 10]
result_list = create_new_list(list1, list2)
print("New list:", result_list)
